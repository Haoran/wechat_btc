.detail {
  background-color: #EFEFF4;
  margin-top: 30rpx;
  margin-bottom: 10rpx;
}

.detail-table {
  background-color: #fff;
}

.detail-info {
  display: flex;
  height: 80rpx;
  margin: 0 20rpx;
  justify-content: flex-start;
  align-items: center;
  border-bottom: solid 1px #f5f5f5;
}

.detail-left {
  width: 35%;
  text-align: left;
}

.detail-right {
  width: 65%;
  text-align: right;
  font-size: 12px;
  color: #aaa;
  word-break: break-all;
}
.detail-address .detail-left{
  width: 28%;
}
.detail-address .detail-right{
  width: 75%;
  white-space: nowrap;
  color:#000000;
}

.qrCode-panel{
  height: 700rpx;
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
   background-color: #EFEFF4;
}
.qrCode{
  width:512rpx;
  height:512rpx;
  border:solid 0px red;
  background:#f1f1f1;
}
.qrCode-text{
  display: block;
  font-size: 14px;
}
.link-height{
  color:blue;
  display: block;
  height:80rpx;
  line-height: 80rpx;
}
.blank{
  width:100%;
  height: 700rpx;
  background-color: #EFEFF4
}
.message-title{
  background-color:#EFEFF4;
  border-bottom:0;
  margin:0;
  font-size: 10px;
  padding: 15rpx 30rpx;
  align-items: flex-start;
  letter-spacing: 2rpx;
  height:50rpx;
  color:#9a9a9a;
}